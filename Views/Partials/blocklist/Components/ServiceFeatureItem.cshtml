@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;

@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;

@{
    var content = (ContentModels.ServiceFeatureItem)Model.Content;
}

<div class="feature-box">
    <i class="@content.Value("serviceFeatureItemIcon")"></i>
    <h4>@content.Value("serviceFeatureItemTitle")</h4>
    <p>@content.Value("serviceFeatureItemDescription")</p>
</div>

