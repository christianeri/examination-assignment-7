@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "LayoutWithShowcase.cshtml";
}

@await Html.PartialAsync("_BrandSection")

<section class="feature-section">
    <div class="container">
          <div class="section-border">
              <div class="section-title">@Model.Value("serviceFeaturesSectionTag")</div>
              <div class="feature-grid">
                  <div class="feature-learnmore">
                        <h2>@Model.Value("serviceFeaturesSectionHeading")</h2>
                        <a class="btn-theme" href="@Model.Value("serviceFeaturesSectionButtonURL")">@Model.Value("serviceFeaturesSectionButtonLabel")<i class="fa-solid fa-arrow-up-right"></i></a>
                  </div>
                  
                  <div class="feature-boxes">
                    @Html.GetBlockListHtml(Model.ServiceFeatures)      
                  </div>
                  
              </div>
          </div>
    </div>
</section>
