@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.PageWithShowcase>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
	ViewData["PageTitle"] = @Model.Value("pageTitle");
	ViewData["PageDecription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}

<header>
    <img class="background-lines" src="-images/background-effects/background-lines.svg" alt="">
    @await Html.PartialAsync("_NavSection")
    @await Html.PartialAsync("_ShowcaseSection")
</header>

<main>
    @await Html.PartialAsync("_BrandSection")
</main>